{
  "name": "inkdrop-keymap",
  "version": "8.2.2",
  "description": "Inkdrop's DOM-aware keymap module",
  "main": "./lib/keymap-manager",
  "scripts": {
    "prepare": "npm run clean && npm run compile && npm run lint",
    "clean": "rimraf lib && rimraf api.json",
    "compile": "coffee --no-header --output lib --compile src && babel src --out-dir lib",
    "lint": "coffeelint -r src spec && eslint src spec",
    "test": "electron-mocha --renderer --preload spec/helpers/setup.js spec/*",
    "test-drive": "SUPPRESS_EXIT=true electron-mocha --no-colors --renderer --interactive --preload spec/helpers/setup.js spec/*",
    "ci": "npm run compile && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inkdropapp/inkdrop-keymap.git"
  },
  "bugs": {
    "url": "https://github.com/inkdropapp/inkdrop-keymap/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@craftzdog/fs-plus": "^4.0.1",
    "chokidar": "^3.5.3",
    "clear-cut": "^2",
    "event-kit": "^2.5.3",
    "inkdrop-keyboard-layout": "^2.0.18",
    "property-accessors": "^1",
    "season": "^6.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.22.4",
    "@babel/register": "^7.21.0",
    "chai": "3.5.0",
    "coffee-script": "^1.12.7",
    "coffeelint": "1.16.0",
    "debounce": "1.0.0",
    "electron": "^21",
    "electron-mocha": "^12.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "grunt": "0.4.1",
    "grunt-atomdoc": "1.0.0",
    "grunt-cli": "0.1.8",
    "grunt-contrib-coffee": "0.9.0",
    "grunt-shell": "0.2.2",
    "lolex": "1.4.0",
    "prettier": "^2.8.8",
    "rimraf": "2.2.2",
    "sinon": "1.17.3",
    "space-pencil": "0.3.0",
    "temp": "0.8.3"
  }
}
